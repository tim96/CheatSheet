<div class="container" style="margin-top: 15px;">
    {% for key, post in posts %}
        <div class="row">
            <div class="col-md-12">
                {% if post.text != 'null' %}<h1>{{ post.text }}</h1>{% endif %}
                {{ post.description|raw }}
            </div>
        </div>
    {% endfor %}
</div>