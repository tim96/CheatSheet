<!DOCTYPE html>
<html {% block html_attributes %}class="no-js"{% endblock %}>
<head>
    {% block meta_tags %}
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta charset="UTF-8">
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

    <title>{% block title %}{{ 'layout.title'|trans({}, 'TimCheatSheetBundle') }}{% endblock %}</title>
</head>
<body>

{% block header %}
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('Home') }}">{{ 'layout.project.name'|trans({}, 'TimCheatSheetBundle') }}</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    {% set currentRoute = app.request.attributes.get('_route') %}
                    <li class="{% if currentRoute == ('Home') %}active{% endif %}"><a href="{{ path('Home') }}">{{ 'layout.home'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                    <li class="{% if currentRoute == ('About') %}active{% endif %}"><a href="{{ path('About') }}">{{ 'layout.about'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                    <li class="{% if currentRoute == ('About') %}active{% endif %}"><a href="{{ path('About') }}">{{ 'layout.contribute'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                    <li class="{% if currentRoute == ('Contact') %}active{% endif %}"><a href="{{ path('Contact') }}">{{ 'layout.contact'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                </ul>
                {% if is_granted('ROLE_USER') %}
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="{{ path('sonata_user_security_logout') }}">{{ 'layout.logout'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                    </ul>
                {% else %}
                    <ul class="nav navbar-nav navbar-right">
                        <li class="{% if currentRoute == ('register') %}active{% endif %}"><a href="{{ path('sonata_user_registration_register') }}">{{ 'layout.register'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                        <li class="{% if currentRoute == ('login') %}active{% endif %}"><a href="{{ path('sonata_user_security_login') }}">{{ 'layout.login'|trans({}, 'TimCheatSheetBundle') }}</a></li>
                    </ul>
                {% endif %}
            </div>
        </div>
    </nav>
{% endblock %}

{% block body %}{% endblock %}

<div class="contentContainer">
    {% block moreContent %}{% endblock %}
</div>

{% block footer %}
    {# <footer  class="footer" style="vertical-align: middle;">
        <div class="container">
            <p class="text-muted">&copy; </p>
        </div>
    </footer> #}
{% endblock %}

{% block stylesheets %}
    {% stylesheets
    '@TimCheatSheetBundle/Resources/public/css/font-awesome.4.4.0.min.css'
    '@TimCheatSheetBundle/Resources/public/css/cosmo/bootstrap.min.css' filter='cssrewrite'
    '@TimCheatSheetBundle/Resources/public/css/site.css' output='css/compiled/app.css' %}
    {#<link rel="stylesheet" href="{{ asset_url }}" />#}
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {% javascripts
    '@TimCheatSheetBundle/Resources/public/js/jquery.min.js'
    '@TimCheatSheetBundle/Resources/public/js/site.js'
    '@TimCheatSheetBundle/Resources/public/js/anchor.min.js'
    '@TimCheatSheetBundle/Resources/public/js/bootstrap.min.js' output='js/compiled/app.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

<script type="text/javascript">
    $(document).ready(function(){
        var stylesheet = document.createElement('link');
        stylesheet.href = '/css/compiled/app.css';
        stylesheet.rel = 'stylesheet';
        stylesheet.type = 'text/css';
        document.getElementsByTagName('head')[0].appendChild(stylesheet);
    });

    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', '{{ google_analytics }}', 'auto');
    ga('send', 'pageview');

</script>

</body>
</html>
<noscript><link rel="stylesheet" href="/css/compiled/app.css"></noscript>